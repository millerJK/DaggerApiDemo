<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
          package="com.cnepay.dragger2">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".DepActivity"
            android:label="@string/title_activity_dependence"
            android:theme="@style/AppTheme.NoActionBar">
        </activity>
        <activity android:name=".LazyAndProviderActivity">
        </activity>
        <activity android:name=".SubActivity">
        </activity>
        <activity android:name=".QualiferActivity">
        </activity>
        <activity android:name=".daggerAndroid.DaggerAndroidActivity">
        </activity>
        <activity android:name=".daggerAndroid.DaggerAndroid2Activity">
        </activity>
        <activity android:name=".lifecycle.UserActivity">
        </activity>
        <activity android:name=".BaseMvpActivity">
        </activity>
        <activity android:name=".SplashActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN"/>

                <category android:name="android.intent.category.LAUNCHER"/>
            </intent-filter>
        </activity>
        <activity android:name=".InjectActivity"/>
    </application>

</manifest>